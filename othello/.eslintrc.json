{
  "env": {
    "browser": true,
    "es2021": true,
    "jest": true
  },
  "extends": ["airbnb", "prettier"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "semi": 0,
    "arrow-parens": 0,
    "comma-dangle": "off",
    "react/no-unescaped-entities": 0,
    "react/jsx-props-no-spreading": 1
  }
}
