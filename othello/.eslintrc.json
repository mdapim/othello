{
  "env": {
    "browser": true,
    "es2021": true,
    "jest": true
  },
  "extends": ["airbnb", "prettier"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "semi": 0,
    "arrow-parens": 0,
    "comma-dangle": "off",
    "react/no-unescaped-entities": 0,
    "react/jsx-props-no-spreading": 1,
    "import/no-extraneous-dependencies": 1,
    "jsx-a11y/label-has-associated-control": 1,
    "no-useless-concat": "off"
  }
}
